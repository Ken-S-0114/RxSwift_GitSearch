target 'RxSwift_GitSearch' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for RxSwift_GitSearch
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'RxDataSources'
  pod 'RxAlamofire/RxCocoa'
  pod 'ObjectMapper'

  target 'RxSwift_GitSearchTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxBlocking'
    pod 'RxTest'
  end

  target 'RxSwift_GitSearchUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
